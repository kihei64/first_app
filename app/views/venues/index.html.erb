<h1>Listing venues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>Address3</th>
    <th>Lat</th>
    <th>Lon</th>
    <th>Maplink</th>
    <th>Contact</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @venues.each do |venue| %>
  <tr>
    <td><%= venue.name %></td>
    <td><%= venue.address1 %></td>
    <td><%= venue.address2 %></td>
    <td><%= venue.address3 %></td>
    <td><%= venue.lat %></td>
    <td><%= venue.lon %></td>
    <td><%= venue.maplink %></td>
    <td><%= venue.contact %></td>
    <td><%= venue.phone %></td>
    <td><%= link_to 'Show', venue %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Venue', new_venue_path %>
